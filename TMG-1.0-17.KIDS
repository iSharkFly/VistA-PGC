"RTN","TMGSTUT2",193,0)
        ;
"RTN","TMGSTUT2",194,0)
        NEW RESULT SET RESULT=0
"RTN","TMGSTUT2",195,0)
        KILL LINEARRAY
"RTN","TMGSTUT2",196,0)
        IF ($GET(STR)="")!($GET(WIDTH)'>0) GOTO SPDONE
"RTN","TMGSTUT2",197,0)
        NEW INDEX SET INDEX=0
"RTN","TMGSTUT2",198,0)
