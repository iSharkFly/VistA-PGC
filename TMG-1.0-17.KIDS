CHECKTYP(GRAPHTYP,TMGERR)        ;
"RTN","TMGGRC2A",56,0)
       SET GRAPHTYP=$GET(GRAPHTYP)
"RTN","TMGGRC2A",57,0)
       IF ("INF;CH;WHO"'[$P(GRAPHTYP,"-",1))!("HT;HC;WT;BMI;WT4L;WT4S;LN;BA;HA;WA;WL;WS"'[$P(GRAPHTYP,";",2)) DO
"RTN","TMGGRC2A",58,0)
       . SET TMGERR="<bold>Error</bold>: Invalid chart type request: '"_GRAPHTYP_"'"
"RTN","TMGGRC2A",59,0)
       QUIT
"RTN","TMGGRC2A",60,0)
       ;
