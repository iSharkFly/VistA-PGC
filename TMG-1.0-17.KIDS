"RTN","TMGGRC1",54,0)
        NEW GRAPH,RESULT
"RTN","TMGGRC1",55,0)
        IF (+$GET(AGEYR)*12)<24 SET GRAPH="LENGTH BY AGE -- INFANT"
"RTN","TMGGRC1",56,0)
        ELSE  SET GRAPH="STATURE BY AGE"
"RTN","TMGGRC1",57,0)
        NEW MONTHS SET MONTHS=+$GET(AGEYR)*12
"RTN","TMGGRC1",58,0)
        SET RESULT=$$COMMON(GRAPH,.LEN,.MONTHS,.GENDER,.NONULL) ;
"RTN","TMGGRC1",59,0)
        QUIT RESULT
