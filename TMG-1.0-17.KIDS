"RTN","TMGTIUO4",103,0)
        . SET RESULT="(No wts available)"
"RTN","TMGTIUO4",104,0)
        ;
"RTN","TMGTIUO4",105,0)
        NEW ARRAY
"RTN","TMGTIUO4",106,0)
