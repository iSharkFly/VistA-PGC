        . IF (TMGNUMLINES>1)&(TMGHEADERBASED=0) DO
"RTN","TMGUSRI2",69,0)
        . . SET TMGWIDTH=(TMGTLEN\TMGNUMLINES)+4
"RTN","TMGUSRI2",70,0)
        . . IF TMGWIDTH<TMGHLEN SET TMGWIDTH=TMGHLEN
"RTN","TMGUSRI2",71,0)
        . IF TMGWIDTH>75 SET TMGWIDTH=75
"RTN","TMGUSRI2",72,0)
        ;
"RTN","TMGUSRI2",73,0)
