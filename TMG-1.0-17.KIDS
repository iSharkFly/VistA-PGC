"RTN","TMGTIUO4",295,0)
        NEW RESULT SET RESULT=""
"RTN","TMGTIUO4",296,0)
        NEW KEYNAME SET KEYNAME=""
"RTN","TMGTIUO4",297,0)
        SET SPACES=$GET(SPACES)
"RTN","TMGTIUO4",298,0)
        NEW TABLESPACE SET TABLESPACE=" "  ;"//kt need to figure how to have this work differently for text vs html
