        IF $DATA(ARRAY("KEY-VALUE",UPKEY))>0 GOTO AIADONE
"RTN","TMGTIUO3",423,0)
        ;
"RTN","TMGTIUO3",424,0)
        SET ARRAY("KEY-VALUE",UPKEY)=$GET(VALUE)
"RTN","TMGTIUO3",425,0)
        NEW LINE SET LINE="        "_$GET(KEY)_" "_$GET(PIVOT)_" "_$GET(VALUE)
"RTN","TMGTIUO3",426,0)
        SET ARRAY("KEY-VALUE",UPKEY,"LINE")=LINE
"RTN","TMGTIUO3",427,0)
        ;
"RTN","TMGTIUO3",428,0)
AIADONE QUIT
"RTN","TMGTIUO3",429,0)
